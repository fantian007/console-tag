{
  "private": false,
  "name": "@sprit/pretty-console-webpack-plugin",
  "author": "赵永盛<1562743134@qq.com>",
  "keywords": [
    "console tags",
    "build version",
    "console debug",
    "控制台打印",
    "构建版本",
    "控制台标签"
  ],
  "version": "0.1.7",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "images",
    "README.MD"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fantian007/pretty-console-webpack-plugin"
  },
  "homepage": "https://github.com/fantian007/pretty-console-webpack-plugin",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "preview": "cd example && yarn start",
    "prepublish": "yarn build"
  },
  "peerDependencies": {
    "react": ">=16",
    "webpack": "^5.28.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/b.esm.js",
  "size-limit": [
    {
      "path": "dist/b.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/b.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/webpack": "^5.28.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "safe-require": "^1.0.4",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "webpack": "5.28.0"
  }
}
